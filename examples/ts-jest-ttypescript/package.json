{
  "name": "ts-jest-ttypescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:runInBand": "jest -i",
    "clearCache": "jest --clearCache"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>config.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/src/.*|\\.(test))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    }
  },
  "devDependencies": {
    "@types/jest": "24.0.24",
    "jest": "24.9.0",
    "jest-ts-auto-mock": "1.0.7",
    "ts-jest": "24.2.0",
    "ttypescript": "1.5.8",
    "typescript": "3.7.4"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ts-auto-mock": "1.4.3"
  }
}
