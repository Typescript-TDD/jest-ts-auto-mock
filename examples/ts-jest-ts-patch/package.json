{
  "name": "ts-jest-ttypescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:runInBand": "jest -i",
    "ts-patch:install": "ts-patch install -s",
    "ts-patch:check": "ts-patch check",
    "ts-patch:help": "ts-patch /?",
    "clearCache": "jest --clearCache"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>config.ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/src/.*|\\.(test))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-patch": "^1.4.2",
    "typescript": "^4.3.5"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jest-ts-auto-mock": "file:../.."
  }
}
